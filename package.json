{
  "name": "wooliesx",
  "version": "1.0.0",
  "description": "Wooliesx API coding exercises",
  "main": "babel-node index.js",
  "scripts": {
    "build": "rm -rf ./build && ./node_modules/.bin/babel ./src -d ./dist --source-maps --copy-files",
    "start": "node ./dist/index.js",
    "dev": "yarn build && yarn start",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "./node_modules/.bin/ava --verbose",
    "lint": "./node_modules/.bin/eslint ./src"
  },
  "engines": {
    "node": "10.15.3"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ChristianRich/wooliesx.git"
  },
  "ava": {
    "require": [
      "@babel/register",
      "@babel/polyfill"
    ]
  },
  "author": "Christian Rich",
  "license": "ISC",
  "homepage": "http://chrisrich.io",
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/plugin-proposal-async-generator-functions": "^7.2.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.0",
    "@babel/preset-stage-3": "^7.0.0",
    "@babel/register": "^7.4.4",
    "ava": "^2.1.0",
    "babel-eslint": "^7.2.3",
    "babel-runtime": "^6.26.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-html-reporter": "^0.5.2",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-prettier": "^3.1.0",
    "prettier": "^1.18.2"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.17.2",
    "express": "^4.15.3",
    "lodash": "^4.17.21",
    "qs": "^6.7.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7"
  }
}
